{
    "name": "Discraft",
    "description": "Play Minecraft in Discord",
    "version": "1.0.0",
    "main": "index.js",
    "license": "undefined",
    "private": "false",
    "author": {
        "name": "Galitan-dev",
        "url": "https://github.com/Galitan-dev"
    },
    "repository": "https://github.com/Galitan-dev/Discraft",
    "bugs": "https://github.com/Galitan-dev/Discraft/issues",
    "homepage": "https://github.com/Galitan-dev/Discraft",
    "scripts": {
        "build": "tsc && shx cp -r src/res dist/res",
        "start": "node dist/index.js",
        "dev": "nodemon",
        "docker": "docker-compose -f docker/docker-compose.yml up -d --build"
    },
    "devDependencies": {
        "@types/node": "^17.0.23",
        "nodemon": "^2.0.15",
        "shx": "^0.3.4",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.14.1",
        "typescript": "^4.6.3"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
        "ext": "ts, js, map"
    },
    "dependencies": {
        "@discordjs/builders": "^0.13.0",
        "@discordjs/rest": "^0.4.1",
        "discord-api-types": "^0.31.2",
        "discord.js": "^13.6.0"
    }
}
